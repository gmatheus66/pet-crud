<p id="notice"><%= notice %></p>

<h1>Vacina Pets</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th>Id seq</th>
      <th>Descricao</th>
      <th>Preco</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @vacina_pets.each do |vacina_pet| %>
      <tr>
        <td><%= vacina_pet.nome %></td>
        <td><%= vacina_pet.id_seq %></td>
        <td><%= vacina_pet.descricao %></td>
        <td><%= vacina_pet.preco %></td>
        <td><%= link_to 'Show', vacina_pet %></td>
        <td><%= link_to 'Edit', edit_vacina_pet_path(vacina_pet) %></td>
        <td><%= link_to 'Destroy', vacina_pet, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Vacina Pet', new_vacina_pet_path %>
